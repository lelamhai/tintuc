                    div.pagination
                        .text-cout 1-15 of 1,438 Results
                        ul.list-link.list-unstyled
                            li: a.first(href="")
                            li: a.item.active(href="") 1
                            li: a.item(href="") 2
                            li: a.item(href="") 3
                            li: a.item(href="") 4
                            li: a.item(href="") 5
                            li: a.last(href="")

    div(class="modal fade model-custom", id="email-agent", tabindex="-1", role="dialog", aria-labelledby="myModalLabel")
        div(class="modal-dialog", role="document")
            div(class="modal-content", style="width: 650px")
                span.close-popup(aria-hidden="true", data-dismiss="modal")
                div(class="modal-body")
                    h3.title-model Contact this Agent
                    form.form-horizontal.custom-from
                        input(type="hidden", id="title_article")
                        div.row
                            div.col-xs-4
                                div
                                    div.text-label Name
                                    input(type="email" class="form-control" id="frist-name" placeholder="Name")
                            div.col-xs-4
                                div
                                    div.text-label Phone
                                    input(type="email" class="form-control" id="frist-name" placeholder="Phone")
                            div.col-xs-4
                                div
                                    div.text-label Email
                                    input(type="email" class="form-control" id="frist-name" placeholder="Email")
                        div.m-t-15
                            div.text-label Send the message
                            textarea.form-control(name="content", rows="5")
                        .clearfix
                        div.form-group.submit-button.text-center.m-t-20
                            button.esta-button.btn.medium(type="submit") Ok
                        div.text-center By sending, you agree to <b class="text-danger">Esta's Terms of Use </b> and <b class="text-danger">Privacy Policy</b>
