extends user_template.jade
block content
    div.container.m-t-30.m-b-30
        div.row
            div.col-xs-3
                div.entry-sidebar
                    div.content-block.sidebar-user
                        div.avatar
                            img(src="asset/img/data-example/avatar.png")
                        div.username DINH MANH C
                        div.type-user User
                        ul.menu-user.list-unstyled
                            li: a(href="") Basic Info
                            li: a.active(href="") Purchased Properties
                            li: a(href="") Saved Homes
                            li: a(href="") Saved Searchs
                            li: a(href="") Messages
                            li: a(href="") Settings

            div.col-xs-9.single-user
                +breadcrumbs(['User','PURCHASED PROPERTIES'])
                h2.title-block.title-page PURCHASED PROPERTIES
                div.des-title You added <b class="text-danger"> 5 </b> properties to Saved Homes. Showing <b class="text-danger"> 1-15.</b>
                div.sort-wrap.m-t-20
                    span.text-intro Sort by
                    div.content-sort
                        div.item
                            - var data = ["15","15 results"]
                            - var list = {30:{parent:"30 results"},45:{parent:"45 results"},60:{parent:"60 results"}}
                            +customSelect(data,list)
                        div.item
                            - var data = ["1","Esta Recommened"]
                            - var list = {2:{parent:"item"}}
                            +customSelect(data,list)
                        div.item
                            - var data = ["Price","Price"]
                            - var list = {2:{parent:"item"}}
                            +customSelect(data,list)
                        div.item
                            - var data = ["project","Project Name"]
                            - var list = {2:{parent:"item"}}
                            +customSelect(data,list)
                        .clearfix
                div.list-style2
                    div.wrap-list
                        div.item
                            div.thumbnail
                                .favorite.active
                                img(src="asset/img/data-example/search1.png", alt="")
                            div.summary
                                h3.title Vinhomes Times City-A7-88
                                    div.price.line-icon
                                        span.icon: img(src="asset/img/icon/ic-dola.png", alt="")
                                        |300.000
                                div.address
                                    .line-icon
                                        span.icon: img(src="asset/img/icon/ic-location-small.png", alt="")
                                        |Dist. 2, HCMC
                                div.properties-entry
                                    div.row-box
                                        div.col-xs-4
                                            .line-icon
                                                span.icon: img(src="asset/img/icon/ic-floor-black.png", alt="")
                                                |Floor 9st
                                        div.col-xs-4
                                            .line-icon
                                                span.icon: img(src="asset/img/icon/ic-size-black.png", alt="")
                                                |50m2
                                        div.col-xs-4
                                            .line-icon
                                                span.icon: img(src="asset/img/icon/ic-bed-black.png", alt="")
                                                |2 bedrooms
                                        div.col-xs-4
                                            .line-icon
                                                span.icon: img(src="asset/img/icon/ic-orientation-black.png", alt="")
                                                |South East
                                        div.col-xs-4
                                            .line-icon
                                                span.icon: img(src="asset/img/icon/ic-sea-view-black.png", alt="")
                                                |Sea view
                                        div.col-xs-4
                                            .line-icon
                                                span.icon: img(src="asset/img/icon/ic-bath-black.png", alt="")
                                                |1 bathroom


                                a.esta-button.btn More detail
                                div.action-item
                                    a.send-email-agent.line-icon(href="", data-title="Vinhomes Times City", data-toggle="modal", data-target="#email-agent",aria-hidden="true", data-dismiss="modal")
